<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.naclo.dao.TicketViewDao">


    <!--查询全部-->
    <select id="queryByUsername" resultType="com.naclo.pojo.TicketView">
        select*
        from dbo.ticketView
        where username = #{username}
    </select>
</mapper>